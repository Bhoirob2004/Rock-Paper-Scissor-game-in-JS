<body>
    <script>
        // Rock Paper Scissor game
        
        let S,P,R;
        S=P=R=5
        let m=0
        let n =0
        for(let i=0; i<5; i++)
        {
            let y = Math.floor(Math.random() *10 + 1);
            let a = prompt("S = Scissors , R = Rock , P = Paper")
            if(y>5)
            {
                m++
            }
            else{
                n++
            }

            
        }
        if(m>n)
        {
            alert("You lose. Try again. Your score is "+ n +" computer score is "+ m)
        }
        else{
            alert("You are the winner. Your score is "+ n +" computer score is "+ m)
        }
    </script>
